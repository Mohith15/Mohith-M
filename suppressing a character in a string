#include<stdio.h>
#include<string.h>
int main()
{
    char *str,ch,*temp;
    int size;
//    str=calloc(1,20);
//    __fpurge(stdin);
    printf("enter the size of the string:");
    scanf("%d",&size);
    __fpurge(stdin);
    printf("enter the character to be suppressed:");
        scanf("%c",&ch);  
    str=calloc(1,20);
        __fpurge(stdin);
    gets(str);
    temp=str;
    for(temp;*temp;temp++)
    {
        if(*temp==ch)
        {
            memmove(temp,temp+1,strlen(temp+1)+1);
            temp--;
        }
    }
    puts(str);
}
